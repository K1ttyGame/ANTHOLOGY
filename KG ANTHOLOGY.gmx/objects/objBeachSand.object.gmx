        //draw_background_tiled_ext(bGlow, 200, 200, 2, 2, c_white, .5)
    }
    
    draw_set_color_write_enable(false,false,false,true);
    draw_set_alpha(1);
    with(objBlock) {
        if (object_index != objPlatform &amp;&amp; object_index != objFallingLeaf &amp;&amp; object_index != objTelescopingBlockSpawn &amp;&amp; object_index != objMovingShootBlock) {
                draw_set_color(c_white)
                draw_rectangle(x, y, x+sprite_width-1, y+sprite_height-1, false)
                if (other.mode == 0) {
                    var i;
                    for (i = x; i &lt; x+sprite_width; i += 32)
                        draw_sprite(sprWhite, 0, i, y-32)
                }
            }
        }
    if (mode == 2) {
        with (objSpikeUp) draw_self()
        with (objSpikeRight) draw_self()
        with (objSpikeLeft) draw_self()
        with (objSpikeDown) draw_self()
    }
    draw_set_color_write_enable(true,true,true,true);
    surface_reset_target()
    draw_surface(sandSurf, 0, 0)
    }

