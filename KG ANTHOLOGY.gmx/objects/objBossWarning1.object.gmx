            <string>timer++; //increment timer
if !audio_is_playing (sndBossAlarm) {
    timer = 0;
    bgAccelerator = 0;
    bgY = 0;
    phase++
}

if (phase &lt; 2) and (timer%5 == 0) {
    //typing code
    if (characters &lt; messageLength) {
    characters += increase; //increase speed based on hold
    messageDraw = string_copy(message, 0, characters); //copy string to current character
    audio_play_sound(sndTypewriter, 10, false) //play le epic typewriter sound
    } 
}

else if (phase &gt;= 2) and (timer%5 == 0) {
    if (characters &gt; 0) {
    characters -= increase; //increase speed based on hold
    messageDraw = string_copy(message, 0, characters); //copy string to current character
    audio_play_sound(sndTypewriter, 10, false) //play le epic typewriter sound
    }
}

if (phase &lt; 1) {
    //lerp strip code
    botStripY = lerp(botStripY, view_hview/2+30, .05) //used to be room_height
    topStripY = lerp(topStripY, view_hview/2-55, .05) //same...
}
else if (phase == 1) {
    //keep strip code for transition out
    botStripY = view_hview/2+32; //used to be room_height
    topStripY = view_hview/2-57; //used to be room_height
}

else if (phase &gt; 1) {
    stripAcc += .5
    botStripY += stripAcc;
    topStripY -= stripAcc;
}

//move warning strips
stripX += 2
if stripX &gt; 128
    stripX = 2;
    
if (phase == 3) {
    instance_destroy();
    instance_create (0, 0, objBossWarning2)
    instance_create(0, 0, bossCreate)
    }
