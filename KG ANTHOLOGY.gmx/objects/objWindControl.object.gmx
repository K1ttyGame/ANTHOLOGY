        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Particles!
stormSys = part_system_create()
part_system_depth(stormSys, -10000)

snowstormPart = part_type_create()
part_type_shape(snowstormPart,  pt_shape_sphere)
part_type_size(snowstormPart, 4, 16, 0, 1)
part_type_color_mix(snowstormPart, $d4c2ae, $e2d3c7) //before, it was color 1 with the first hex code
if (windMode != 0) {
    part_type_alpha3(snowstormPart, .6, .4, .6)
    part_type_speed(snowstormPart, 40,50, 0, 0)
    if (windMode == -1)
        part_type_direction(snowstormPart, 160, 200, 0, 1)
    if (windMode == 1)
        part_type_direction(snowstormPart, -20, 20, 0, 1)
    part_type_life(snowstormPart, 150*(room_width/800), 150*(room_width/800))
    }
else {
    part_type_alpha3(snowstormPart, .05, .02, .05)
    part_type_speed(snowstormPart, 4, 6, 0, 0)
    part_type_direction(snowstormPart, 250, 290, 0, 1)
    part_type_life(snowstormPart, 500, 500)
}

snowstormEmit = part_emitter_create(stormSys)
if (windMode == -1)
    part_emitter_region(stormSys, snowstormEmit, room_width+128, room_width+128, -96, room_height+96, ps_shape_line, ps_distr_linear)
    
if (windMode == 1)
    part_emitter_region(stormSys, snowstormEmit, -128, -128, -96, room_height+96, ps_shape_line, ps_distr_linear)
    
if (windMode == 0)
    part_emitter_region(stormSys, snowstormEmit, -128, room_width+128, -96, -96, ps_shape_line, ps_distr_linear)
    
part_emitter_stream(stormSys, snowstormEmit, snowstormPart, -1)

repeat (room_speed*3) {
    part_system_update(stormSys)
    }
</string>
