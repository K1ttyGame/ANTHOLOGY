            <kind>1</kind>
            <string>if (hspeed == 0) {
    mode = 0
    image_speed = 0
    timer = 0
    }

else {
    mode = 1
    image_speed = .2
    if (timer &lt; 5)
        timer++
    }
    
//collision code
if !(place_free(x+hspeed, y) || place_meeting(x + hspeed, y, objShellBlock)) {
    hspeed = -hspeed
}

vspeed += .2;

if ((!place_free(x, y + vspeed) || place_meeting(x, y + vspeed, objShellBlock)) &amp;&amp; vspeed != 0) {
    var maxDist = abs(vspeed);
    var dir = 270 - 180 * (vspeed &lt; 0);
    move_contact_solid(dir, maxDist);
    vspeed = 0;
}

if (activeTimer == 0) {
    if (mode == 0) {
        if (place_meeting(x + hspeed, y + vspeed, objPlayer)) { //used to be no hspeed or vspeed
            if (objPlayer.x &lt;= x+16)
                hspeed = moveSpeed
            if (objPlayer.x &gt; x+16)
                hspeed = -moveSpeed
            //with objPlayer  {vspeed = -8;y--}
            activeTimer = 8
            audio_play_sound(sndShellBounce, 10, false)
            objPlayer.djump = 1
        }
    }
    
    if (mode == 1) {
        if (timer &gt;= 5) {
            if (place_meeting(x + hspeed,y + vspeed, objPlayer)) { //used to be 15, used to be no hspeed or vspeed
                hspeed = 0
                with objPlayer  {vspeed = -8;y--}
                audio_play_sound(sndShellBounce, 10, false)
                activeTimer = 8
                objPlayer.djump = 1
            }
            
            else if (place_meeting(x+1 + hspeed, y + vspeed, objPlayer) || place_meeting(x-1 + hspeed, y + vspeed, objPlayer)) { //used to be no hspeed or vspeed
                scrKillPlayer()
            }
        }
    }
}

else {
    activeTimer--
}

vspeed = clamp(vspeed,  0, 6)
</string>
